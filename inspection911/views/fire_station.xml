<odoo>

  <record model="ir.ui.view" id="inspection911.fire_station_list">
    <field name="name">inspection911 fire station list</field>
    <field name="model">fire_station</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="user_ids"/>
        <field name="location_ids"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="inspection911.fire_station_form">
    <field name="name">inspection911 fire station form</field>
    <field name="model">fire_station</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="user_ids"/>
            <field name="location_ids">
              <tree editable="bottom">
                <field name="name"/>
              </tree>
            </field>
          </group>
        </sheet>
      </form>
    </field>
  </record>

</odoo>