<odoo>

  <record model="ir.ui.view" id="inspection911.device_type_list">
    <field name="name">inspection911 device type list</field>
    <field name="model">inspection911.device_type</field>
    <field name="arch" type="xml">
      <tree editable="bottom">
        <field name="name"/>
      </tree>
    </field>
  </record>

  <record model="ir.ui.view" id="inspection911.device_type_form">
    <field name="name">inspection911 device type form</field>
    <field name="model">inspection911.device_type</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>

</odoo>