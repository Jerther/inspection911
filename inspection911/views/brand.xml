<odoo>

  <record model="ir.ui.view" id="inspection911.brand_list">
    <field name="name">inspection911 brand list</field>
    <field name="model">brand</field>
    <field name="arch" type="xml">
      <tree>
        <field name="name"/>
        <field name="make_ids"/>
      </tree>
    </field>
  </record>

    <record model="ir.ui.view" id="inspection911.brand_form">
    <field name="name">inspection911 brand form</field>
    <field name="model">brand</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="make_ids">
              <tree editable="bottom">
                <field name="name"/>
              </tree>
            </field>
          </group>

        </sheet>
      </form>
    </field>
  </record>

</odoo>